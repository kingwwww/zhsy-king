        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.enter.LoginActivity" android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.enter.RegisterActivity">
        </activity>
        <activity android:name=".activity.store.StoreIndexActivity">
        </activity>
        <activity android:name=".activity.store.StoreSelfActivity">
        </activity>
        <activity android:name=".activity.store.ManageClassfiyActivity">
        </activity>
        <activity android:name=".activity.store.AddProductActivity">
        </activity>
        <activity android:name=".activity.store.PreViewActivity">
        </activity>
        <activity android:name=".activity.user.UserIndexActivity">
        </activity>
        <activity android:name=".activity.user.ShoppingActivity">
        </activity>
        <activity android:name=".activity.user.UserSelfActivity">
        </activity>
        <activity android:name=".activity.user.SettleActivity">
        </activity>
        <activity android:name=".activity.store.StoreOrderActivity">
        </activity>
        <activity android:name=".activity.user.ManageAddressActivity">
        </activity>
    </application>

</manifest>